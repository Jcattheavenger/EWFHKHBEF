--<Aimbot>--
local AimbotSettings = {
    Aimbot = true,
    Aiming = false,
    AimbotSpeed = 0,
    AimBone = "Head",
    TeamCheck = true
}

UserInputService.InputBegan:Connect(function(Input)
    if Input.KeyCode == AimbotKey then
        AimbotSettings.Aiming = true
    end
end)

UserInputService.InputEnded:Connect(function(Input)
    if Input.KeyCode == AimbotKey then
        AimbotSettings.Aiming = false
    end
end)

game:GetService("RunService").RenderStepped:Connect(function()

    local distance = math.huge
    local target = nil

        if AimbotEnabled then
        if AimbotSettings.Aiming then
            for _,v in pairs (game:GetService("Players"):GetChildren()) do
                if AimbotSettings.TeamCheck then
                    if v ~= Plr and v.Team ~= Plr.Team and v.Character and v.Character:FindFirstChild("Humanoid") then
                        if v.Character:FindFirstChild("Humanoid").Health > 0 and v.Character:FindFirstChild("Head") then

                            local plr = v.Character
                            local plrhead, onscreen = game.Workspace.CurrentCamera:WorldToViewportPoint(plr[ActiveBone].Position)

                            if onscreen then

                                local magnitude = (Vector2.new(Plr:GetMouse().X, Plr:GetMouse().Y) - Vector2.new(plrhead.X, plrhead.Y)).magnitude

                                if magnitude < distance then
                                    distance = magnitude
                                    target = plr
                                end
                            end
                        end
                    end
                else
                    if v ~= Plr and v.Character and v.Character:FindFirstChild("Humanoid") then
                        if v.Character:FindFirstChild("Humanoid").Health > 0 and v.Character:FindFirstChild("Head") then

                            local plr = v.Character
                            local plrhead, onscreen = game.Workspace.CurrentCamera:WorldToViewportPoint(plr.Head.Position)

                            if onscreen then

                                local magnitude = (Vector2.new(Plr:GetMouse().X, Plr:GetMouse().Y) - Vector2.new(plrhead.X, plrhead.Y)).magnitude

                                if magnitude < distance then
                                    distance = magnitude
                                    target = plr
                                end
                            end
                        end
                    end
                end
            end
        end

        if target ~= nil and target:FindFirstChild("Humanoid") and target:FindFirstChild("Head") then
            if target:FindFirstChild("Humanoid").Health > 0 then

                local Tween = game.TweenService:create(game.Workspace.CurrentCamera, TweenInfo.new(AimbotSettings.AimbotSpeed), {CFrame = CFrame.new(game.Workspace.CurrentCamera.CFrame.Position, target[ActiveBone].Position)})
                Tween:Play()
                UserInputService.InputEnded:Connect(function(Input)
                    if Input.KeyCode == Enum.KeyCode.E then
                        Tween:Cancel() return end
                end)
            end
        end
    end
end)
--<End>--

