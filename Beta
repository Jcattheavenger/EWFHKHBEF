--<Buttons>--
function CreateTabButton(Variable, vName, Index, vText)
    Variable.Name = vName
    Variable.AnchorPoint = VectorCenter
    Variable.Position = UDim2.new(0.485, 0, Index, 0)
    Variable.Size = UDim2.new(0, 122, 0, 60)
    Variable.BorderSizePixel = 3
    Variable.BackgroundColor3 = lBlack
    Variable.BorderColor3 = CurrentColor
    Variable.BorderSizePixel = 1
    Variable.BorderMode = Enum.BorderMode.Inset
    Variable.Text = vText
    Variable.TextColor3 = lGrey
    Variable.TextSize = 15
end

CreateTabButton(LegitTabButton, "LegitTabButton", 0.25, "Legit")
CreateTabButton(RageTabButton, "RageTabButton", 0.4, "Rage")
CreateTabButton(VisualsTabButton, "VisualsTabButton", 0.55, "Visuals")
CreateTabButton(MiscTabButton, "MiscTabButton", 0.7, "Misc")
CreateTabButton(ColorsTabButton, "ColorsTabButton", 0.85, "Colors")
--<End>--

--<Tab Button Functions>--

LegitTabButton.MouseButton1Click:Connect(function()
    if CurrentMode == lBlack then
        LegitTabButton.TextColor3 = White
    else
        LegitTabButton.TextColor3 = Black
    end
    RageTabButton.TextColor3 = lGrey
    VisualsTabButton.TextColor3 = lGrey
    MiscTabButton.TextColor3 = lGrey
    ColorsTabButton.TextColor3 = lGrey

    HomeFrame.Visible = false
    LegitTab.Visible = true
    RageTab.Visible = false
    VisualsTab.Visible = false
    MiscTab.Visible = false
    ColorsTab.Visible = false
end)

RageTabButton.MouseButton1Click:Connect(function()
    LegitTabButton.TextColor3 = lGrey
    if CurrentMode == lBlack then
        RageTabButton.TextColor3 = White
    else
        RageTabButton.TextColor3 = Black
    end
    VisualsTabButton.TextColor3 = lGrey
    MiscTabButton.TextColor3 = lGrey
    ColorsTabButton.TextColor3 = lGrey

    HomeFrame.Visible = false
    LegitTab.Visible = false
    RageTab.Visible = true
    VisualsTab.Visible = false
    MiscTab.Visible = false
    ColorsTab.Visible = false
end)

VisualsTabButton.MouseButton1Click:Connect(function()
    LegitTabButton.TextColor3 = lGrey
    RageTabButton.TextColor3 = lGrey
    if CurrentMode == lBlack then
        VisualsTabButton.TextColor3 = White
    else
        VisualsTabButton.TextColor3 = Black
    end
    MiscTabButton.TextColor3 = lGrey
    ColorsTabButton.TextColor3 = lGrey

    HomeFrame.Visible = false
    LegitTab.Visible = false
    RageTab.Visible = false
    VisualsTab.Visible = true
    MiscTab.Visible = false
    ColorsTab.Visible = false
end)

MiscTabButton.MouseButton1Click:Connect(function()
    LegitTabButton.TextColor3 = lGrey
    RageTabButton.TextColor3 = lGrey
    VisualsTabButton.TextColor3 = lGrey
    if CurrentMode == lBlack then
        MiscTabButton.TextColor3 = White
    else
        MiscTabButton.TextColor3 = Black
    end
    ColorsTabButton.TextColor3 = lGrey

    HomeFrame.Visible = false
    LegitTab.Visible = false
    RageTab.Visible = false
    VisualsTab.Visible = false
    MiscTab.Visible = true
    ColorsTab.Visible = false
end)

ColorsTabButton.MouseButton1Click:Connect(function()
    LegitTabButton.TextColor3 = lGrey
    RageTabButton.TextColor3 = lGrey
    VisualsTabButton.TextColor3 = lGrey
    MiscTabButton.TextColor3 = lGrey
    if CurrentMode == lBlack then
        ColorsTabButton.TextColor3 = White
    else
        ColorsTabButton.TextColor3 = Black
    end

    HomeFrame.Visible = false
    LegitTab.Visible = false
    RageTab.Visible = false
    VisualsTab.Visible = false
    MiscTab.Visible = false
    ColorsTab.Visible = true
end)
--<End>--

