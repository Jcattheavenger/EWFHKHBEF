function AddUIButton(Variable, vName, PosX, PosY, vText)
    Variable.Name = vName
    Variable.AnchorPoint = VectorCenter
    Variable.Position = UDim2.new(PosX, 0, PosY, 0)
    Variable.Size = UDim2.new(0, 60, 0, 25)
    Variable.BorderColor3 = CurrentColor
    Variable.BackgroundColor3 = lBlack
    Variable.TextScaled = true
    Variable.BorderSizePixel = 2
    Variable.ZIndex = 1
    Variable.TextColor3 = White
    Variable.Font = Enum.Font.RobotoCondensed
    Variable.Text = vText
end

AddUIButton(SkinChangerButton, "SkinChangerButton", 0.8, 0.4, "Skin Changer")
AddUIButton(KillAllButton, "KillAllButton", 0.2, 0.6, "Mod Weapon")
AddUIButton(MenuColorDrop, "MenuColorDrop", 0.2, 0.2, "Menu Color")
AddUIButton(CrosshairColorDrop, "CrosshairColorDrop", 0.8, 0.2, "Crosshair")
AddUIButton(NameEspColorDrop, "NameEspColorDrop", 0.8, 0.4, "Names")
AddUIButton(BoxEspColorDrop, "BoxEspColorDrop", 0.2, 0.6, "Boxes")
AddUIButton(ChamsEspColorDrop, "ChamsEspColorDrop", 0.8, 0.6, "Chams")
AddUIButton(RemoveRecoilButton, "RemoveRecoilButton", 0.2, 0.6, "Remove Recoil")

function CreateScroll(Variable, vName, vSize)
    Variable.Name = vName
    Variable.AnchorPoint = VectorCenter
    Variable.Size = UDim2.new(0, 65, 0, 75)
    Variable.Position = UDim2.new(0.5, 0, 2.5, 0)
    Variable.ScrollBarThickness = 5
    Variable.CanvasSize = UDim2.new(0, 0, vSize, 0)
    Variable.BackgroundColor3 = lBlack
    Variable.BorderColor3 = CurrentColor
    Variable.BorderSizePixel = 2
    Variable.Visible = false
end

CreateScroll(MenuScroll, "MenuScroll", 5)
CreateScroll(CrosshairScroll, "CrosshairScroll", 7)
CreateScroll(NameEspScroll, "NameEspScroll", 7)
CreateScroll(BoxEspScroll, "BoxEspScroll", 7)
CreateScroll(ChamsEspScroll, "ChamsEspScroll", 10)

