function AddUICheckBox(Variable, vName, PosX, PosY, vText)
    Variable.Name = vName
    Variable.AnchorPoint = VectorCenter
    Variable.Position = UDim2.new(PosX, 0, PosY, 0)
    Variable.Size = UDim2.new(0, 25, 0, 25)
    Variable.BorderColor3 = CurrentColor
    Variable.BackgroundColor3 = lBlack
    Variable.Text = vText
    Variable.TextScaled = true
    Variable.TextColor3 = CurrentColor
end

