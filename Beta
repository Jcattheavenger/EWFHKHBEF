EspChamsToggle.MouseButton1Click:Connect(function()
    ChamsEnabled = not ChamsEnabled

    print(ChamsEnabled)

    wait(0.5)

    if ChamsEnabled == true then
        EspChamsToggle.Text = "✓"
    else
        EspChamsToggle.Text = ""
        for i,v in pairs (game:GetService("Players"):GetPlayers()) do
            if v ~= game:GetService("Players").LocalPlayer and v.Character then
                for i,v in pairs (v.Character:GetChildren()) do
                    if v:IsA("MeshPart") or v.Name == "Head" or v.Name == "Torso" or v.Name == "Right Arm" or v.Name == "Left Arm" or v.Name == "Right Leg" or v.Name == "Left Leg" then
                        for i,v in pairs(v:GetDescendants()) do
                            if v:IsA("SurfaceGui") then
                                v:Destroy()
                            end
                        end
                    end
                end
            end
        end
    end
end)

function CreateChams(vName, vParent, vFace)
    local ChamsGui = Instance.new("SurfaceGui", vParent)
    ChamsGui.Parent = vParent
    ChamsGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    ChamsGui.Face = Enum.NormalId[vFace]
	ChamsGui.LightInfluence = 0
	ChamsGui.ResetOnSpawn = false
    ChamsGui.Name = vName
    ChamsGui.AlwaysOnTop = true
    local Chams = Instance.new("Frame",ChamsGui)
	Chams.BackgroundColor3 = ChamsColor
    Chams.BorderSizePixel = 0
	Chams.Size = UDim2.new(1,0,1,0)
    Chams.BackgroundTransparency = ChamsOpacity
end

game:GetService("RunService").RenderStepped:Connect(function()
    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if TeamCheck == true then
            if v ~= game:GetService("Players").LocalPlayer and v.TeamColor ~= game:GetService("Players").LocalPlayer.TeamColor and v.Character ~= nil and ChamsEnabled == true and v.Character:FindFirstChild("Head") and v.Character.Head:FindFirstChild("Chams")== nil then
                for i,v in pairs (v.Character:GetChildren()) do
                    if v:IsA("MeshPart") or v.Name == "Head" or v.Name == "Torso" or v.Name == "Right Arm" or v.Name == "Left Arm" or v.Name == "Right Leg" or v.Name == "Left Leg" then
                        CreateChams("Chams",v,"Back")
                        CreateChams("Chams",v,"Front")
                        CreateChams("Chams",v,"Left")
                        CreateChams("Chams",v,"Right")
                        CreateChams("Chams",v,"Right")
                        CreateChams("Chams",v,"Top")
                        CreateChams("Chams",v,"Bottom")
                    end
                end
            end     
        else
            if v ~= game:GetService("Players").LocalPlayer and v.Character ~= nil and ChamsEnabled == true and v.Character:FindFirstChild("Head") and v.Character.Head:FindFirstChild("Chams")== nil then
                for i,v in pairs (v.Character:GetChildren()) do
                    if v:IsA("MeshPart") or v.Name == "Head" or v.Name == "Torso" or v.Name == "Right Arm" or v.Name == "Left Arm" or v.Name == "Right Leg" or v.Name == "Left Leg" then
                        CreateChams("Chams",v,"Back")
                        CreateChams("Chams",v,"Front")
                        CreateChams("Chams",v,"Left")
                        CreateChams("Chams",v,"Right")
                        CreateChams("Chams",v,"Right")
                        CreateChams("Chams",v,"Top")
                        CreateChams("Chams",v,"Bottom")
                    end
                end
            end
        end
    end
end)

EspHealthBarToggle.MouseButton1Click:Connect(function()
    HealthEspEnabled = not HealthEspEnabled

    if HealthEspEnabled then
        wait(0.5)
        EspHealthBarToggle.Text = "✓"
    else
        wait(0.5)
        EspHealthBarToggle.Text = ""
    end

    while wait(2) do
    Update()
    end
end)

HealthGui.Name = "HealthGui"
HealthGui.AlwaysOnTop = true
HealthGui.Size = UDim2.new(1, 0, 1, 0)
HealthGui.ExtentsOffset = Vector3.new(-3, 1, 0)

HealthEsp.Name = "Esp"
HealthEsp.BackgroundTransparency = 1
HealthEsp.AnchorPoint = Vector2.new(0.5, 0.5)
HealthEsp.Size = UDim2.new(1, 0, 1, 0)
HealthEsp.Position = UDim2.new(0, 0, 0.5, 0)
HealthEsp.Text = ""
HealthEsp.TextColor3 = Color3.fromRGB(0, 255, 0)
HealthEsp.TextStrokeTransparency = 0

function Update()
    while wait(0.1) do  
        if HealthEspEnabled == true then
            if TeamCheck == true then
                for _,v in pairs(game:GetService("Players"):GetChildren()) do
                    if v ~= game.Players.LocalPlayer and v.Character ~= nil and v.TeamColor ~= game:GetService("Players").LocalPlayer.TeamColor then
                        if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                            if v.Character.HumanoidRootPart:FindFirstChild("HealthGui") == nil then
                                HealthGui.Adornee = v.Character.HumanoidRootPart
                                HealthGui:Clone().Parent = v.Character.HumanoidRootPart
                            end
                        end
                    end
                    if v ~= game.Players.LocalPlayer and v.Character ~= nil and v.TeamColor ~= game:GetService("Players").LocalPlayer.TeamColor then
                        if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                            if v.Character.HumanoidRootPart.HealthGui:FindFirstChild("Esp") then
                                if v.Character:FindFirstChild("Humanoid") then
                                    v.Character.HumanoidRootPart.HealthGui:FindFirstChild("Esp").Text = "%"..math.floor(v.Character.Humanoid.Health)..""
                                end
                            end
                        end
                    end
                end
            else
                for _,v in pairs(game:GetService("Players"):GetChildren()) do
                    if v ~= game.Players.LocalPlayer and v.Character ~= nil then
                        if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                            if v.Character.HumanoidRootPart:FindFirstChild("HealthGui") == nil then
                                HealthGui.Adornee = v.Character.HumanoidRootPart
                                HealthGui:Clone().Parent = v.Character.HumanoidRootPart
                            end
                        end
                    end
                    if v ~= game.Players.LocalPlayer and v.Character ~= nil then
                        if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                            if v.Character.HumanoidRootPart.HealthGui:FindFirstChild("Esp") then
                                if v.Character:FindFirstChild("Humanoid") then
                                    v.Character.HumanoidRootPart.HealthGui:FindFirstChild("Esp").Text = "%"..math.floor(v.Character.Humanoid.Health)..""
                                end
                            end
                        end
                    end
                end
            end
        else
            wait(0.5)
            for _,v in pairs(game:GetService("Players"):GetChildren()) do
                if v.Character ~= nil then
                    if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                        if v.Character.HumanoidRootPart:FindFirstChild("HealthGui") ~= nil then
                            v.Character.HumanoidRootPart.HealthGui:Destroy()
                        end
                    end
                end
            end
        end
    end
end

