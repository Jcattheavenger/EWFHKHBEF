    end)
end)()

--<Console>--
function CreateLine(vText, vColor, PosY, vParent)
    NewLine = Instance.new("TextLabel",vParent)

    NewLine.Name = "Line "..NameIndex..""
    NewLine.AnchorPoint = Vector2.new(0.5, 0.5)
    NewLine.Position = UDim2.new(0.5, 0, PosY, 0)
    NewLine.Size = UDim2.new(0.95, 0, 0.05, 0)
    NewLine.BackgroundTransparency = 1
    NewLine.TextWrapped = true
    NewLine.TextXAlignment = Enum.TextXAlignment.Left
    NewLine.TextColor3 = Color3.new(vColor, vColor, vColor)
    NewLine.TextSize = 10
    NewLine.Text = vText

    return NewLine
end

CreateLine("- cmds for commands", 0.5, PosIndex, ConsoleSpace)
PosIndex = PosIndex + 0.02

