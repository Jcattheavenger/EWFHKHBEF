--<Checkbox Functions>--
_G.aimbot = false
AimbotToggle.MouseButton1Click:Connect(function()
    AimbotEnabled = not AimbotEnabled
    print(AimbotEnabled)

    if AimbotEnabled then
        AimbotToggle.Text = "✓"
        _G.aimbot = true
    else
        AimbotToggle.Text = ""
        _G.aimbot = false
    end
end)

--<Aimbot>--
function getClosest()
    local dist = math.huge
    local target = nil
    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if AimbotTeamCheck then
            if v ~= game:GetService("Players").LocalPlayer then
                if v.Character and v.Character:FindFirstChild("Head") and v.Team ~= game:GetService("Players").LocalPlayer.Team and _G.aimbot and v.Character:FindFirstChild("Humanoid") then
                    if v.Character.Humanoid.Health > 0 then
                        local magnitude = (v.Character.Head.Position - game:GetService("Players").LocalPlayer.Character.Head.Position).magnitude
                        if magnitude < dist then
                            dist = magnitude
                            target = v
                        end
                    end
                end
            end
        else
            if v ~= game:GetService("Players").LocalPlayer then
                if v.Character and v.Character:FindFirstChild("Head") and _G.aimbot and v.Character:FindFirstChild("Humanoid") then
                    if v.Character.Humanoid.Health > 0 then
                        local magnitude = (v.Character.Head.Position - game:GetService("Players").LocalPlayer.Character.Head.Position).magnitude
                        if magnitude < dist then
                            dist = magnitude
                            target = v
                        end
                    end
                end
            end
        end
    end
    return target
end

getClosest()    

coroutine.wrap(function()
    while wait() do
        if AimbotEnabled then
            game.UserInputService.InputBegan:Connect(function(input)
                if input.KeyCode == AimbotKey then
                _G.aim = true
                while wait() do
                        local Tween = game.TweenService:create(game.workspace.CurrentCamera, TweenInfo.new(AimbotSpeed), {CFrame = CFrame.new(game.workspace.CurrentCamera.CFrame.Position, getClosest().Character.Head.Position)})
                        Tween:Play()
                        if _G.aim == false then Tween:Cancel() return end
                    end
                end
            end)
            game.UserInputService.InputEnded:Connect(function(input)
                if input.KeyCode == AimbotKey then
                    _G.aim = false
                end
            end)
            wait()
        else
            _G.aim = false
        end
    end
end)()
--<End>--

