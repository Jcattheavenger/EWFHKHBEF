    elseif AimbotSmoothing.Text == "4" then
        AimbotSpeed = 1
    elseif AimbotSmoothing.Text == "5" then
        AimbotSpeed = 1.25
    else
        CreateNotification("Value must be 0-5", 5)
    end
end)

