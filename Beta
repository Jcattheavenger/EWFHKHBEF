    WelcomeLabel.TextColor3 = CurrentText
    WatermarkBackground.TextColor3 = CurrentText
    ChangeLogButton.TextColor3 = CurrentText
    WatermarkLabel.TextColor3 = CurrentText
    ChatSpamLabel.TextColor3 = CurrentText
    DrawCrosshairLabel.TextColor3 = CurrentText
    SkinChangerButton.TextColor3 = CurrentText
