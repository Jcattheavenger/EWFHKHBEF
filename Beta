LightModeToggle.MouseButton1Click:Connect(function()
    if LightModeToggle.Text == "" then
        LightModeToggle.Text = "✓"
    else
        LightModeToggle.Text = ""
    end

    LightMode = not LightMode

    if CurrentMode == lBlack then
        CurrentMode = dWhite
        CurrentText = lBlack
        ColorsTabButton.TextColor3 = Black
    else
        CurrentMode = lBlack
        CurrentText = White
        ColorsTabButton.TextColor3 = White
    end

    i = 2
    i2 = 2
    LightModeUpdate()
    TextColorUpdate()
    i = 1
    i2 = 1
    if LightMode then
        PlrAvatarBackground.BackgroundColor3 = Color3.fromRGB(215, 215, 215)
    else
        PlrAvatarBackground.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
    end
end)

EspNamesToggle.MouseButton1Click:Connect(function()
    EspEnabled = not EspEnabled

    wait(0.5)

    print(EspEnabled)

    if EspEnabled == false then
        EspNamesToggle.Text = ""
        for i,v in pairs (game:GetService("Players"):GetPlayers()) do
            if v ~= game:GetService("Players").LocalPlayer and v.Character ~= nil then
                if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                    if v.Character.HumanoidRootPart:FindFirstChild("NameEsp") ~= nil then
                        v.Character.HumanoidRootPart.NameEsp:Destroy()
                    end
                end
            end
        end
    else
        EspNamesToggle.Text = "✓"
    end
end)

