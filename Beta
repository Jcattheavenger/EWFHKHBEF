    MiscTab.BorderColor3 = CurrentColor
    ColorsTab.BorderColor3 = CurrentColor
    RageTabButton.BorderColor3 = CurrentColor
    LegitTabButton.BorderColor3 = CurrentColor
    MiscTabButton.BorderColor3 = CurrentColor
    ColorsTabButton.BorderColor3 = CurrentColor
    VisualsTabButton.BorderColor3 = CurrentColor
    VisualsDrawLine.BackgroundColor3 = CurrentColor
    LegitDrawLine.BackgroundColor3 = CurrentColor
    RageDrawLine.BackgroundColor3 = CurrentColor
    MiscDrawLine.BackgroundColor3 = CurrentColor
    ColorsDrawLine.BackgroundColor3 = CurrentColor
    MenuPink.BorderColor3 = CurrentColor
    MenuYellow.BorderColor3 = CurrentColor
    MenuGreen.BorderColor3 = CurrentColor
    MenuColorDrop.BorderColor3 = CurrentColor
    MenuBlue.BorderColor3 = CurrentColor
    WatermarkToggle.TextColor3 = CurrentColor
    WatermarkToggle.BorderColor3 = CurrentColor
    DrawCrosshair.BorderColor3 = CurrentColor
    DrawCrosshair.TextColor3 = CurrentColor
    ChatSpamToggle.BorderColor3 = CurrentColor
    ChatSpamToggle.TextColor3 = CurrentColor
    CrosshairColorDrop.BorderColor3 = CurrentColor
    CrosshairRed.BorderColor3 = CurrentColor
    CrosshairGreen.BorderColor3 = CurrentColor
    CrosshairBlue.BorderColor3 = CurrentColor
    CrosshairWhite.BorderColor3 = CurrentColor
    CrosshairPink.BorderColor3 = CurrentColor
    CrosshairYellow.BorderColor3 = CurrentColor
    CrosshairBlack.BorderColor3 = CurrentColor
