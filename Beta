ConsoleSpace.CanvasSize = UDim2.new(0, 0, 4, 0)

function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

counter = 0

function rainbow()
    coroutine.wrap(function()
        while wait(0.1)do
        CurrentColor = Color3.fromHSV(zigzag(counter),1,1)
        ColorUpdate()
        counter = counter + 0.01
        end
    end)()
end

CmdTextBox.Name = "CmdTextBox"
CmdTextBox.AnchorPoint = Vector2.new(0.5, 0.5)
CmdTextBox.Position = UDim2.new(0.5, 0, 0.95, 0)
CmdTextBox.Size = UDim2.new(0.9, 0, 0.05, 0)
CmdTextBox.BackgroundTransparency = 1
CmdTextBox.TextColor3 = Color3.new(1, 1, 1)
CmdTextBox.TextSize = 10
CmdTextBox.TextXAlignment = Enum.TextXAlignment.Left
CmdTextBox.TextWrapped = true
CmdTextBox.Text = ""
CmdTextBox.PlaceholderText = "Enter cmd here..."
CmdTextBox.FocusLost:Connect(function()
    CreateLine("- "..CmdTextBox.Text.."", 1, PosIndex, ConsoleSpace)
    NameIndex = NameIndex + 1
    PosIndex = PosIndex + 0.02
    CmdTextBox.Text = ""

    if NewLine.Text == "- clear" then

        for _,v in pairs (ConsoleSpace:GetChildren()) do
            v:Destroy()
            PosIndex = 0.02
        end

    elseif NewLine.Text == "- cmds" then

        CommandsList.Visible = true

    elseif NewLine.Text == "- shadow" then

        CurrentColor = Shadow
        i = 2
        i2 = 2
        ColorUpdate()
        i = 1
        i2 = 1

    elseif NewLine.Text == "- refresh names" then

        RefreshNameEsp()

    elseif NewLine.Text == "- refresh boxes" then

        RefreshBoxEsp()

    elseif NewLine.Text == "- refresh chams" then

        UpdateChams()

    elseif NewLine.Text == "- refresh health" then

        RefreshHealthEsp()

    elseif NewLine.Text == "- crash" then

        while true do
        end

    elseif NewLine.Text == "- unload" then

        Folder:Destroy()
        CustomFovEnabled = false
        game.Workspace.CurrentCamera.FieldOfView = 70
        ChatSpam = false
        BoxEspEnabled = false
        HealthEspEnabled = false
        EspEnabled = false
        ChamsEnabled = false
        AimbotEnabled = false

    elseif NewLine.Text == "- rainbow" then

        rainbow()
        
    end
end)

