function CreateTextBox(Variable, vName, vText, PosX, PosY)
    Variable.Name = vName
    Variable.AnchorPoint = VectorCenter
    Variable.Position = UDim2.new(PosX, 0, PosY, 0)
    Variable.Size = UDim2.new(0, 25, 0, 25)
    Variable.BorderColor3 = CurrentColor
    Variable.BackgroundColor3 = lBlack
    Variable.Text = vText
    Variable.TextScaled = true
    Variable.TextColor3 = White
end

CreateTextBox(ChamsOpacityTextBox, "ChamsOpacityTextBox", 5, 0.5, 2)
CreateTextBox(FovChangerTextBox, "FovChangerTextBox", 70, 0.5, 2)
CreateTextBox(AimbotSmoothing, "AimbotSmoothing", 0, Left, 0.4)
CreateTextBox(AimbotKeyTextBox, "AimbotKeyTextBox", "e", Right, 0.4)
CreateTextBox(ThirdPersonDistanceTextBox, "ThirdPersonDistanceTextBox", 10, 0.5, 2)
CreateTextBox(AntiAimMaxText, "AntiAimMaxText", 360, 0.5, 2)
CreateTextBox(AntiAimMinText, "AntiAimMinText", -360, 0.5, 2)

--<TextBox Functions>--
AACheck = true

AntiAimMaxText.FocusLost:Connect(function()
    AAMaxAngle = tonumber(AntiAimMaxText.Text)
    if AAMaxAngle > AAMinAngle then
        CreateNotification("Max angle is "..AAMaxAngle.."", 5)
        AACheck = true
    else
        CreateNotification("Attempt to set AAMaxAngle < AAMinAngle", 5)
        AACheck = false
    end
end)
AntiAimMinText.FocusLost:Connect(function()
    AAMinAngle = tonumber(AntiAimMinText.Text)
    if AAMinAngle < AAMaxAngle then
        CreateNotification("Min angle is "..AAMinAngle.."", 5)
        AACheck = true
    else
        CreateNotification("Attempt to set AAMinAngle > AAMaxAngle", 5)
        AACheck = false
    end
end)

ThirdPersonDistanceTextBox.FocusLost:Connect(function()
    if tonumber(ThirdPersonDistanceTextBox.Text) <= 120 then
        if tonumber(ThirdPersonDistanceTextBox.Text) >= 1 then
            ThirdPersonDistance = tonumber(ThirdPersonDistanceTextBox.Text)
        else
            CreateNotification("Value is under 1!", 5)
        end
    else
        CreateNotification("Value is over 120!", 5)
    end
end)

i = 1
ChamsOpacityTextBox.FocusLost:Connect(function()
    if ChamsOpacityTextBox.Text == "5" then
        ChamsOpacity = 0
    elseif ChamsOpacityTextBox.Text == "4" then
        ChamsOpacity = 0.2
    elseif ChamsOpacityTextBox.Text == "3" then
        ChamsOpacity = 0.4
    elseif ChamsOpacityTextBox.Text == "2" then
        ChamsOpacity = 0.6
    elseif ChamsOpacityTextBox.Text == "1" then
        ChamsOpacity = 0.8
    else
        CreateNotification("Value must be 1-5", 5)
    end

    UpdateChams()
end)

i2 = 1
FovChangerTextBox.FocusLost:Connect(function()
    CustomFov = FovChangerTextBox.Text
    CreateNotification("Refreshing Fov Value...", 1.5)
    UpdateFov()
end)

AimbotSmoothing.FocusLost:Connect(function()
    if AimbotSmoothing.Text == "0" then
        AimbotSettings.AimbotSpeed = 0.01
    elseif AimbotSmoothing.Text == "1" then
        AimbotSettings.AimbotSpeed = 0.25
    elseif AimbotSmoothing.Text == "2"  then
        AimbotSettings.AimbotSpeed = 0.5
    elseif AimbotSmoothing.Text == "3" then
        AimbotSettings.AimbotSpeed = 0.75
    elseif AimbotSmoothing.Text == "4" then
        AimbotSettings.AimbotSpeed = 1
    elseif AimbotSmoothing.Text == "5" then
        AimbotSettings.AimbotSpeed = 1.25
    else
        CreateNotification("Value must be 0-5", 5)
    end
end)

AimbotKeyTextBox.FocusLost:Connect(function()
    if AimbotKeyTextBox.Text == "a" then
        AimbotKey = Enum.KeyCode.A
    elseif AimbotKeyTextBox.Text == "b" then
        AimbotKey = Enum.KeyCode.B
    elseif AimbotKeyTextBox.Text == "c" then
        AimbotKey = Enum.KeyCode.C
    elseif AimbotKeyTextBox.Text == "d" then
        AimbotKey = Enum.KeyCode.D
    elseif AimbotKeyTextBox.Text == "e" then
        AimbotKey = Enum.KeyCode.E
    elseif AimbotKeyTextBox.Text == "f" then
        AimbotKey = Enum.KeyCode.F
    elseif AimbotKeyTextBox.Text == "g" then
        AimbotKey = Enum.KeyCode.G
    elseif AimbotKeyTextBox.Text == "h" then
        AimbotKey = Enum.KeyCode.H
    elseif AimbotKeyTextBox.Text == "i" then
        AimbotKey = Enum.KeyCode.I
    elseif AimbotKeyTextBox.Text == "j" then
        AimbotKey = Enum.KeyCode.J
    elseif AimbotKeyTextBox.Text == "k" then
        AimbotKey = Enum.KeyCode.K
    elseif AimbotKeyTextBox.Text == "l" then
        AimbotKey = Enum.KeyCode.L
    elseif AimbotKeyTextBox.Text == "m" then
        AimbotKey = Enum.KeyCode.M
    elseif AimbotKeyTextBox.Text == "n" then
        AimbotKey = Enum.KeyCode.N
    elseif AimbotKeyTextBox.Text == "o" then
        AimbotKey = Enum.KeyCode.O
    elseif AimbotKeyTextBox.Text == "p" then
        AimbotKey = Enum.KeyCode.P
    elseif AimbotKeyTextBox.Text == "q" then
        AimbotKey = Enum.KeyCode.Q
    elseif AimbotKeyTextBox.Text == "r" then
        AimbotKey = Enum.KeyCode.R
    elseif AimbotKeyTextBox.Text == "s" then
        AimbotKey = Enum.KeyCode.S
    elseif AimbotKeyTextBox.Text == "t" then
        AimbotKey = Enum.KeyCode.T
    elseif AimbotKeyTextBox.Text == "u" then
        AimbotKey = Enum.KeyCode.U
    elseif AimbotKeyTextBox.Text == "v" then
        AimbotKey = Enum.KeyCode.V
    elseif AimbotKeyTextBox.Text == "w" then
        AimbotKey = Enum.KeyCode.W
    elseif AimbotKeyTextBox.Text == "x" then
        AimbotKey = Enum.KeyCode.X
    elseif AimbotKeyTextBox.Text == "y" then
        AimbotKey = Enum.KeyCode.Y
    elseif AimbotKeyTextBox.Text == "z" then
        AimbotKey = Enum.KeyCode.Z
    else
        CreateNotification("Value must be letter", 5)
    end
end)
--<End>--

function CreateLabel(Variable, vName, vText)
    Variable.Name = vName
    Variable.AnchorPoint = Vector2.new(0, 0.5)
    Variable.Position = UDim2.new(1.2, 0, 0.5, 0)
    Variable.Size = UDim2.new(0, 125, 0, 25)
    Variable.BackgroundTransparency = 1
    Variable.TextColor3 = White
    Variable.TextSize = 25
    Variable.TextXAlignment = Left
    Variable.Font = Enum.Font.RobotoCondensed
    Variable.Text = vText
end

