function CrosshairUpdate()
    DrawnCrosshair.ImageColor3 = CrosshairColor
end

UserInputService.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.RightShift then
        Menu.Enabled = not Menu.Enabled
	end
end)

function RefreshHealthEsp()
    HealthEspEnabled = not HealthEspEnabled

    CreateNotification("Refreshing Health...", 1.5)

    wait(0.5)

    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if v ~= game:GetService("Players").LocalPlayer and v.Character ~= nil then
            if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                if v.Character.HumanoidRootPart:FindFirstChild("HealthGui") ~= nil then
                    v.Character.HumanoidRootPart.HealthGui:Destroy()
                end
            end
        end
    end

    wait(0.5)
    HealthEspEnabled = not HealthEspEnabled

    if HealthEspEnabled == true then
        EspHealthBarToggle.Text = "✓"
    else
        EspHealthBarToggle.Text = ""
    end
end

function RefreshNameEsp()
    EspEnabled = not EspEnabled

    CreateNotification("Refreshing Names...", 1.5)

    wait(0.5)

    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if v ~= game:GetService("Players").LocalPlayer and v.Character ~= nil then
            if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                if v.Character.HumanoidRootPart:FindFirstChild("NameEsp") ~= nil then
                    v.Character.HumanoidRootPart.NameEsp:Destroy()
                end
            end
        end
    end

    wait(0.5)
    EspEnabled = not EspEnabled

    if EspEnabled == true then
        EspNamesToggle.Text = "✓"
    else
        EspNamesToggle.Text = ""
    end
end

function UpdateChams()
    ChamsEnabled = not ChamsEnabled

    CreateNotification("Refreshing Chams...", 1.5)

    wait(0.5)

    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if v ~= game:GetService("Players").LocalPlayer and v.Character then
            for i,v in pairs (v.Character:GetChildren()) do
                if v:IsA("MeshPart") or v.Name == "Head" or v.Name == "Torso" or v.Name == "Right Arm" or v.Name == "Left Arm" or v.Name == "Right Leg" or v.Name == "Left Leg" then
                    for i,v in pairs(v:GetDescendants()) do
                        if v:IsA("SurfaceGui") then
                            v:Destroy()
                        end
                    end
                end
            end
        end
    end

    wait(0.5)
    ChamsEnabled = not ChamsEnabled

    if ChamsEnabled == true then
        EspChamsToggle.Text = "✓"
    else
        EspChamsToggle.Text = ""
    end
end

function RefreshBoxEsp()
    BoxEspEnabled = not BoxEspEnabled

    CreateNotification("Refreshing Boxes...", 1.5)

    wait(0.5)

    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if v ~= game:GetService("Players").LocalPlayer and v.Character ~= nil then
            if v.Character:FindFirstChild("HumanoidRootPart") then
                if v.Character.HumanoidRootPart:FindFirstChild("BoxEsp") then
                    v.Character.HumanoidRootPart.BoxEsp:Destroy()
                end
            end
        end
    end

    wait(0.5)
    BoxEspEnabled = not BoxEspEnabled

    if BoxEspEnabled == true then
        EspBoxesToggle.Text = "✓"
    else
        EspBoxesToggle.Text = ""
    end
end

function ChatSpamFunction()
    while ChatSpam do
        if game.PlaceId == 301549746 then
            game:GetService("ReplicatedStorage").Events.PlayerChatted:FireServer(ChatTable[math.random(1,#ChatTable)], false, "Innocent", false, true)
            wait(3)
        else
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ChatTable[math.random(1, #ChatTable)], "All")
            wait(3)
        end
    end
end

function UpdateFov()
    coroutine.wrap(function()
        while CustomFovEnabled do
            game.Workspace.CurrentCamera.FieldOfView = CustomFov
            wait(0.2)
        end
   end)()
end

function UpdateTime()
    Seconds = Seconds + 1

    if Seconds == 60 then
        Minutes = Minutes + 1
        Seconds = 0
    end

    if Minutes == 60 then
        Hours = Hours + 1
        Minutes = 0
    end

    WatermarkBackground.Text = "Ninelives "..Version.." | "..Plr.Name.." | "..Hours..":"..Minutes..":"..Seconds..""
end

coroutine.wrap(function()
    while wait(1) do
        UpdateTime()
    end
end)()
