local Version = "v2.0.4"

function CreateNotification(vText, vDuration, vButton)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Ninelives "..Version.."",
        Text = vText,
        Duration = vDuration,
        Button1 = vButton
    })
end

