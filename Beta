local Version = "v1.8.9"

function CreateNotification(vText, vDuration, vButton)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Ninelives "..Version.."",
        Text = vText,
        Duration = vDuration,
        Button1 = vButton
    })
end

