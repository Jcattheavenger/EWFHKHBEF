function CreateGui(Variable, vName)
    Variable.Name = vName
    Variable.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    Variable.ResetOnSpawn = false
end

CreateGui(Menu, "Menu")
CreateGui(Watermark, "Watermark")
CreateGui(Crosshair, "Crosshair")

