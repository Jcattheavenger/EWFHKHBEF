function CreateTab(Variable, vName, vVisible)
    Variable.Name = vName
    Variable.Position = UDim2.new(1, 0, 0, 0)
    Variable.Size = UDim2.new(0, 400, 0, 600)
    Variable.BorderSizePixel = 3
    Variable.BackgroundColor3 = lBlack
    Variable.BorderColor3 = CurrentColor
    Variable.Active = true
    Variable.Visible = vVisible
end

CreateTab(HomeFrame, "HomeFrame", true)

function CreateHeader(Variable, vName, vText, PosY, SizeY)
    Variable.Name = vName
    Variable.AnchorPoint = VectorCenter
    Variable.Position = UDim2.new(0.5, 0, PosY, 0)
    Variable.Size = UDim2.new(0, 350, 0, SizeY)
    Variable.BackgroundTransparency = 1
    Variable.TextScaled = true
    Variable.TextColor3 = White
    Variable.Font = Enum.Font.RobotoCondensed
    Variable.Text = vText
end

CreateHeader(HomeHeader, "HomeHeader", "Ninelives "..Version.."", 0.1, 75)

function CreateDrawLine(Variable, vName)
    Variable.Name = vName
    Variable.AnchorPoint = VectorCenter
    Variable.Position = UDim2.new(0.5, 0, 1.5)
    Variable.Size = UDim2.new(0, 350, 0, 5)
    Variable.BorderSizePixel = 0
    Variable.BackgroundColor3 = CurrentColor
end

CreateDrawLine(HomeDrawLine, "HomeDrawLine")

--<Console>--
function CreateLine(vText, vColor, PosY, vParent)
    NewLine = Instance.new("TextLabel",vParent)

    NewLine.Name = "Line "..NameIndex..""
    NewLine.AnchorPoint = Vector2.new(0.5, 0.5)
    NewLine.Position = UDim2.new(0.5, 0, PosY, 0)
    NewLine.Size = UDim2.new(0.95, 0, 0.05, 0)
    NewLine.BackgroundTransparency = 1
    NewLine.TextWrapped = true
    NewLine.TextXAlignment = Enum.TextXAlignment.Left
    NewLine.TextColor3 = Color3.new(vColor, vColor, vColor)
    NewLine.TextSize = 10
    NewLine.Text = vText

    return NewLine
end

CreateLine("- cmds for commands", 0.5, PosIndex, ConsoleSpace)
PosIndex = PosIndex + 0.02

