NameEsp.Position = UDim2.new(0.5, 0, 0, 0)
NameEsp.Font = "GothamSemibold"
NameEsp.TextSize = 11
NameEsp.TextStrokeTransparency = 0
NameEsp.TextStrokeColor3 = Black

game:GetService("RunService").RenderStepped:Connect(function()
    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if TeamCheck == true then
            if v ~= game:GetService("Players").LocalPlayer and v.Character ~= nil and EspEnabled == true and v.TeamColor ~= game:GetService("Players").LocalPlayer.TeamColor then
                if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                    if v.Character.HumanoidRootPart:FindFirstChild("NameEsp") == nil then
                        NameEsp.Text = ""..v.Name..""
                        NameEsp.TextColor3 = NameEspColor
                        NameEspGui.Adornee = v.Character.HumanoidRootPart
                        NameEspGui:Clone().Parent = v.Character.HumanoidRootPart
                    end
                end
            end
        else
            if v ~= game:GetService("Players").LocalPlayer and v.Character ~= nil and EspEnabled == true then
                if v.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                    if v.Character.HumanoidRootPart:FindFirstChild("NameEsp") == nil then
                        NameEsp.Text = ""..v.Name..""
                        NameEsp.TextColor3 = NameEspColor
                        NameEspGui.Adornee = v.Character.HumanoidRootPart
                        NameEspGui:Clone().Parent = v.Character.HumanoidRootPart
                    end
                end
            end
        end
    end
end)

