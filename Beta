    CustomFovEnabled = not CustomFovEnabled

    if CustomFovEnabled then
        FovChangerToggle.Text = "✓"
        UpdateFov()
    else
        FovChangerToggle.Text = ""
        game.Workspace.CurrentCamera.FieldOfView = 70
    end
end)
