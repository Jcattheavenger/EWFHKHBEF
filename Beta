i = 1
ChamsOpacityTextBox.FocusLost:Connect(function()
    if ChamsOpacityTextBox.Text == "5" then
        ChamsOpacity = 0
    elseif ChamsOpacityTextBox.Text == "4" then
        ChamsOpacity = 0.2
    elseif ChamsOpacityTextBox.Text == "3" then
        ChamsOpacity = 0.4
    elseif ChamsOpacityTextBox.Text == "2" then
        ChamsOpacity = 0.6
    elseif ChamsOpacityTextBox.Text == "1" then
        ChamsOpacity = 0.8
    else
        CreateNotification("Value must be 1-5", 5)
    end

    UpdateChams()
end)

i2 = 1
FovChangerTextBox.FocusLost:Connect(function()
    CustomFov = FovChangerTextBox.Text
    CreateNotification("Refreshing Fov Value...", 1.5)
    UpdateFov()
end)

