function DropDownElement(Variable, vName, Index, vText)
    Variable.Name = vName
    Variable.Position = UDim2.new(0, 0, Index, 0)
    Variable.Size = UDim2.new(0, 60, 0, 25)
    Variable.BorderSizePixel = 2
    Variable.BackgroundColor3 = lBlack
    Variable.BorderColor3 = CurrentColor
    Variable.TextColor3 = White
    Variable.TextScaled = true
    Variable.Text = vText
    Variable.ZIndex = 2
end

